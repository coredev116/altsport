{
  "name": "altsports-api",
  "version": "1.0.0",
  "description": "altsports-api",
  "author": "Octalogic Tech",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "local:dev": "docker compose -f docker-compose.db.yml up -d && npm run dev",
    "local:dev:down": "docker compose -f docker-compose.db.yml down",
    "dev": "doppler run -- ts-node-dev --respawn --transpile-only --notify=false src/main.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "CI=true eslint src/**/*.{ts,js}",
    "lint:fix": "npm run lint --fix",
    "prettier:format": "prettier --write \"src/**/*.{ts,jsx,js,css,json,css,scss,md}\"",
    "prettier:check": "CI=true npx prettier \"src/**/*.{ts,jsx,js,css,json,css,scss,md}\" --check",
    "dev:migrate:make": "doppler run -- npx ts-node --transpile-only ./node_modules/typeorm/cli.js migration:create",
    "dev:migrate:latest": "doppler run -- npx ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d src/config/datasource.ts",
    "dev:migrate:rollback": "doppler run -- npx ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert -d src/config/datasource.ts",
    "seed:run": "doppler run -- knex seed:run",
    "prod:migrate:latest": "doppler run -- npx typeorm migration:run -d dist/config/datasource.js",
    "prod:migrate:rollback": "doppler run -- npx typeorm migration:revert -d dist/config/datasource.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs-plus/rabbitmq": "^1.4.4",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^9.4.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "argon2": "^0.30.3",
    "aws-sdk": "^2.1494.0",
    "axios": "^1.6.1",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "express": "^4.18.2",
    "firebase-admin": "^11.11.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.10.28",
    "lodash.capitalize": "^4.2.1",
    "lodash.chunk": "^4.2.0",
    "lodash.groupby": "^4.6.0",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "parse-domain": "5.0.0",
    "passport": "^0.6.0",
    "passport-firebase-jwt": "^1.2.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pusher-js": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "sqs-consumer": "5.8.0",
    "twilio": "^3.84.1",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.1",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.17",
    "@types/lodash.capitalize": "^4.2.7",
    "@types/lodash.chunk": "^4.2.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.orderby": "^4.6.7",
    "@types/node": "^18.15.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "envalid": "^7.3.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^41.1.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "knex": "^2.4.2",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "rimraf": "^4.4.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.ts": [
      "npm run-script lint",
      "npx pretty-quick --staged"
    ]
  }
}
